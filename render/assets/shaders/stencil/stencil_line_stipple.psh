
struct PSInput {
    float4 Pos    : SV_POSITION;
    float3 UV     : TEX_COORD;
    float4 Color  : COLOR;
};

struct PSOutput {
    float4 Color : SV_TARGET;
};

void main(in PSInput PSIn, out PSOutput PSOut) {
    if (PSIn.Color.a <= 0.0 /*|| 0.125 < mod(v_stipple, 0.25)*/ ) {
		discard;
	}

    PSOut.Color = PSIn.Color;
}
